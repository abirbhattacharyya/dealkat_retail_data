    <% form_tag product_action_path('search') do %>
        <div class="required">
              <h4><label>Search</label></h4>
              <%= text_field_tag :search, '', :maxLength => 11, :onKeyPress => "return keyRestrict(event,'0123456789')" %>
        </div>
        <%= submit_tag "Go!", :class => "block" %>
    <% end %>

    <% for graph in @graphs %>
       <%= render :partial => "controls/graph", :object => graph.url, :locals => {:id => graph.id} %>
    <% end %>
